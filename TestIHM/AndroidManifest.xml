<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testihm"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.testihm.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.testihm.UploadActivity"
            android:label="@string/title_activity_upload" >
        </activity>
        <activity
            android:name="com.example.testihm.DownloadActivity"
            android:label="@string/title_activity_download" >
        </activity>
        <activity
            android:name="com.example.testihm.OptionsActivity"
            android:label="@string/title_activity_options" >
        </activity>
        <activity
            android:name="com.example.testihm.FragmentList"
            android:label="@string/title_activity_fragment_list" >
        </activity>
        <activity
            android:name="com.example.testihm.OptionsFragActivity"
            android:label="@string/title_activity_options_frag" >
        </activity>
        <activity
            android:name="com.example.testihm.OptionsFragment"
            android:label="@string/title_activity_options_fragment" >
        </activity>
        <activity
            android:name="com.example.testihm.AddLocationStorage"
            android:label="@string/title_activity_add_location_storage" >
        </activity>
        <activity
            android:name="com.example.testihm.DetailedLocationStorage"
            android:label="@string/title_activity_detailed_location_storage" >
        </activity>
        <activity
            android:name="com.example.testihm.Upload2"
            android:label="@string/title_activity_upload2" >
        </activity>
        <activity
            android:name="com.example.testihm.ConfigUploadActivity"
            android:label="@string/title_activity_config_upload" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.testihm.ConnectionActivity"
            android:label="@string/title_activity_connection" >
        </activity>
    </application>

</manifest>
